<Window x:Class="WpfBiblioteka.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfBiblioteka"
        mc:Ignorable="d"
        Title="MainWindow" Height="292" Width="335" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="104,32,0,0" TextWrapping="Wrap" Text="Molimo vas da se ulogujete" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="20,101,0,0" TextWrapping="Wrap" Text="Korisnicko Ime" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock2" HorizontalAlignment="Left" Margin="20,158,0,0" TextWrapping="Wrap" Text="Korisnicka sifra" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="130,96,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Height="23" Margin="130,153,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="buttonLogin" Content="Uloguj se" HorizontalAlignment="Left" Margin="130,196,0,0" VerticalAlignment="Top" Width="75" Click="buttonLogin_Click"/>
    </Grid>
</Window>
